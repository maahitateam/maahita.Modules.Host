    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "prettier": "~2.3.1",
    "prettier-plugin-svelte": "^2.3.0",
    "svelte": "^3.38.2",
    "svelte-check": "^2.1.0",
    "svelte-preprocess": "^4.7.3",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2"
  },
  "type": "module",
  "dependencies": {
    "axios": ">=0.21.1",
    "svelte-persistent-store": "^0.1.6",
